{
  "name": "my-project-devcontainer",
  "image": "mcr.microsoft.com/devcontainers/base:ubuntu",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.10",
      "installTools": false
    }
  },
  "forwardPorts": [8000],
  "postCreateCommand": "python manage.py runserver 0.0.0.0:8000",
  "postCreateMessage": "The development environment is ready.",
  "mounts": ["source=/workspace/DjangoPY110,target=/workspace/DjangoPY110,type=bind,consistency=cached"]
}
